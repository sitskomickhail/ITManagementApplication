<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="models.entities.WorkerProject" table="WorkerProjects" catalog="ITManagementDb">
        <id></id>
        <property name="workerId" type="java.lang.Integer">
            <column name="WorkerId"/>
        </property>
        <property name="projectId" type="java.lang.Integer">
            <column name="ProjectId"/>
        </property>
        <property name="cost" type="java.lang.Double">
            <column name="Cost" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>